{
  "name": "mini-express-project",
  "version": "1.0.0",
  "description": "A simple Express.js application integrated with Socket.io and MongoDB and socket.io",
  "main": "src/server.ts",
  "scripts": {
    "test-socket": " node ./test/test.socket.js",
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/app.ts",
